  "text": "Vous a-t-on diagnostiqué ?",
  "type": "MULTI_CHOICES",
  "choices": [
    {
      "code": "0",
      "text": "Une pathologie thyroïdienne (hypo ou hyperthyroïdie, thyroïdite, cancer, nodules...)"
    },
    {
      "code": "1",
      "text": "Une pathologie parathyroïdienne (hypo ou hyperparathyroïdie, cancer...)"
    },
    {
      "code": "2",
      "text": "Une pathologie de l’hypophyse (hyperprolactinémie, adénome, insuffisance hypophysaire, acromégalie, diabète insipide...)"
    },
    {
      "code": "3",
      "text": "Une pathologie surrénalienne (Cushing, Conn, phéochromocytome, insuffisance surrénalienne...)"
    },
    {
      "code": "4",
      "text": "Une pathologie métabolique (obésité, diabète type 1 ou 2, cholestérol, triglycérides...)"
    },
    {
      "code": "5",
      "text": "Aucun de ces antécédents",
      "exclusive": true
    }
  ],
